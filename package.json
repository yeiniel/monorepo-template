    "@tsconfig/next": "^1.0.5",
    "@tsconfig/node20": "^1.0.1",
    "@tsconfig/recommended": "^1.0.2",
    "@types/eslint": "^8",
    "@types/express": "^4",
    "@types/node": "^20",
    "@types/prettier": "^2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/serverless": "^3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
